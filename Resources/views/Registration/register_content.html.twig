<form method="post" action="{{ path('club_base_register_u2f') }}" id="u2fForm" data-action="reg" data-request='{{ registrationRequest|raw }}'>
    <div id="u2fkeyname">
        <input type="text" name="keyName" id="keyName" placeholder="{{ 'ru2_f_two_factor.name'|trans }}"/>
        <input type="submit" value="{{ 'ru2_f_two_factor.register'|trans }}" />
    </div>
    <div id="u2fpressbutton" style="display: none;">{{ 'ru2_f_two_factor.pressbutton'|trans }}</div>
    <input type="hidden" name="_auth_code" id="_auth_code" />
    <div id="u2fError" style="display: none;"></div>
</form>
<script type="text/javascript" charset="utf-8" src="{{ asset('assets/ru2ftwofactor.js') }}"></script>
